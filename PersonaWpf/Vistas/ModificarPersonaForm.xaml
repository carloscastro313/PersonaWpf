<Window x:Class="PersonaWpf.Vistas.ModificarPersonaForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PersonaWpf.Vistas" xmlns:Componentes="clr-namespace:PersonaWpf.Componentes"
        mc:Ignorable="d"
        Title="Modificar persona" Height="200" Width="300">
    <StackPanel>
        <Componentes:Form x:Name="frmPersona"/>
        <Button Grid.Row="4" Grid.ColumnSpan="2" Content="Modificar"  Width="100" Height="30" Name="btnModificar" Click="btnModificar_Click" />
    </StackPanel>
</Window>
